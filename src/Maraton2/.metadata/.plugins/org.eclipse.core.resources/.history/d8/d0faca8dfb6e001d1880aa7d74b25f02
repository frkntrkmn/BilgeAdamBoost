package maraton2.soru3;

public class Y覺lan extends Surungen{

	
	
	
	
	public Y覺lan() {
		super();
	}

	public Y覺lan(String ad, int kilo, int uzunluk, boolean tehlikeliMi) {
		super(ad, kilo, uzunluk, tehlikeliMi=true);
	}

	@Override
	public void surun() {
		System.out.println("yilan surundu");
		
	}

	@Override
	public void sesCikar() {
		System.out.println("yilan tisladi");
		
	}

	@Override
	public String toString() {
		return "Y覺lan [getAd()=" + getAd() + ", getKilo()=" + getKilo() + ", getUzunluk()=" + getUzunluk()
				+ ", isTehlikeliMi()=" + isTehlikeliMi() + "]";
	}
	
	

}
