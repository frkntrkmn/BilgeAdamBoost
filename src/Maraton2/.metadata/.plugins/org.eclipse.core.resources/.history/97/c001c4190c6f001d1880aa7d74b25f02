		super(ad, soyad, checkin);
		EKONOMIFIYAT = eKONOMIFIYAT;
	}

	public int getEKONOMIFIYAT() {
		return EKONOMIFIYAT;
	}

	public void setEKONOMIFIYAT(int eKONOMIFIYAT) {
		EKONOMIFIYAT = eKONOMIFIYAT;
	}

	
	

