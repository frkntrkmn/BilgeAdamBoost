package maraton2.soru3;

public class Kertenkele extends Surungen {

	
	
	
	
	public Kertenkele() {
		super();
	}

	public Kertenkele(String ad, int kilo, int uzunluk) {
		super(ad, kilo, uzunluk);
		setTehlikeliMi(false);

	}

	@Override
	public void surun() {
		System.out.println("kertenkele surundu");
		
	}

	@Override
	public void sesCikar() {

		System.out.println("kertenkele ses cikardi");
	}

	@Override
	public String toString() {
		return "Kertenkele [getAd()=" + getAd() + ", getKilo()=" + getKilo() + ", getUzunluk()=" + getUzunluk()
				+ ", isTehlikeliMi()=" + isTehlikeliMi() + "]";
	}

	
	
}
