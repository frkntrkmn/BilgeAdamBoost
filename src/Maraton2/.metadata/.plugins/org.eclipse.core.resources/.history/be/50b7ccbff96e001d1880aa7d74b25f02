package maraton2.soru3;

public class Aslan extends MemeliHayvan {

	
	
	
	
	public Aslan() {
	}


	public Aslan(String ad, int kilo, int uzunluk, boolean tehlikeliMi) {
		super(ad, kilo, uzunluk, tehlikeliMi=true);
	}


	@Override
	public void yuru() {
		System.out.println("aslan y端r端d端");
		
	}
	
	
	@Override
	public void sesCikar() {
		System.out.println("aslan k端kredi");
	}

	

	

}
