package maraton2.soru2;

public class Main {
	public static void main(String[] args) {
		
		CustomStringTR customStr = new CustomStringTR("  merhaba--  ");
		System.out.println(customStr.uzunluk());
		System.out.println(customStr.bosMuKontrol());
		System.out.println(customStr.bölme());
		System.out.println(customStr.değerArama());
		System.out.println(customStr.charArrayCevirme());
		System.out.println(customStr.değerKontrol());
		System.out.println(customStr.sonDeğerKontrol());
		System.out.println(customStr.indexGetir());
		System.out.println(customStr.boslukSilme());
		System.out.println(customStr.büyükHarfYapma());
		System.out.println(customStr.kücükHarfYapma());





		
	
	
	}

	

}
